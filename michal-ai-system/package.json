    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "googleapis": "^160.0.0",
    "helmet": "^7.0.0",
    "inquirer": "^9.2.8",
    "joi": "^17.9.2",

{
  "name": "michal-ai-assistant",
  "version": "1.0.0",
  "description": "מערכת עוזר AI אישית חכמה למיכל",
  "main": "server-unified.js",
  "scripts": {
    "start": "node simple-server.js",
    "start-simple": "node simple-server.js",
    "start-legacy": "node server.js",
    "dev": "nodemon server-unified.js",
    "test": "jest",
    "initialize": "node scripts/initialize.js",
    "cli": "node cli.js",
    "migrate": "node cli.js migrate",
    "seed": "node cli.js seed",
    "backup": "node cli.js backup",
    "health": "curl http://localhost:3000/api/health && curl http://localhost:8000/health",
    "logs": "node cli.js logs",
    "user:create": "node cli.js user create",
    "user:list": "node cli.js user list",
    "start:clean": "node server-clean.js",
    "start:agent": "cd ai_agent && python smart_server.py",
    "start:all": "concurrently \"npm run start\" \"npm run start:agent\"",
    "test:agent": "cd ai_agent && python -m pytest",
    "fix": "node fix_life_orchestrator.js"
